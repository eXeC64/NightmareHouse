VertexLitGeneric
{
	$baseTexture models\NH2Zombies\Creeper\Creeper_map
	$bumpmap models\NH2Zombies\Creeper\false_normal
	$lightwarptexture models\NH2Zombies\Creeper\stealth_lightwarp
	$additive 1
	$model 1
	$nocull 1

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 40
		}
	}
}